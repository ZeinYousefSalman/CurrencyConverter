{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    },
    "Settings": {

        "GradesForTwo": "thousand",
        "GradesForThree": "million,thousand",
        "HundredWord": "hundred",
        "AndWord": "and",
        "SingleDigits": "zero,one,two,three,four,five,six,seven,eight,nine,ten,eleven,twelve,thirteen,fourteen,fifteen,sixteen,seventeen,eighteen,ninteen",
        "DoubleDigits": ",,twenty,thirty,fourty,fifty,sixty,seventy,eighty,ninety",
        "SingleIntPartCurrency": "dollar",
        "SingleDecimalPartCurrency": "cent",
        "PluralIntPartCurrency": "dollars",
        "PluralDecimalPartCurrency": "cents"
    }
}
